    "@types/enzyme": "^3.10.18",
    "@types/enzyme-adapter-react-16": "^1.0.9",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
